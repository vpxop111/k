{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "predict.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [{ "src": "/predict", "dest": "index.js" }],
  "env": {
    "PYTHON_VERSION": "3.8"
  }
}
